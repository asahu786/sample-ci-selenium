<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="SampleSuite" verbose="1" parallel="false">

  <!-- Add TestNG listeners (like EmailableReporter2) -->
  <listeners>
    <listener class-name="org.testng.reporters.EmailableReporter2"/>
  </listeners>

  <!-- Define a test group -->
  <test name="UI_Tests">
    <packages>
      <!-- Replace with your actual package containing tests -->
      <package name="com.galaxe.sampleci.ui"/>
    </packages>
  </test>

</suite>